        android:allowBackup="true"
        android:icon="@drawable/se_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.xcedim.sudokuexperiment.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.xcedim.sudokuexperiment.level_selection"
            android:label="Select the difficulty level" />
        <activity
            android:name="com.xcedim.sudokuexperiment.game_display_gx"
